<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="
             http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.2.xsd">

	<bean name="beanA" class="pl.kubiczak.felix.shark.samples.ioc.blueprint.spring.simple.Bean">
		<constructor-arg value="Bean A"/>
		<property name="sampleProperty" value="Sample Value"/>
	</bean>

	<bean name="beanB" class="pl.kubiczak.felix.shark.samples.ioc.blueprint.spring.simple.Bean">
		<constructor-arg value="Bean B"/>
	</bean>

</beans>
