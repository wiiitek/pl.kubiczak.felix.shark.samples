<project>
	<skin>
		<groupId>org.apache.maven.skins</groupId>
		<artifactId>maven-fluido-skin</artifactId>
		<version>1.5</version>
	</skin>

	<custom>
		<fluidoSkin>
			<sideBarEnabled>true</sideBarEnabled>

			<gitHub>
				<projectId>wiiitek/${project.artifactId}</projectId>
				<ribbonOrientation>right</ribbonOrientation>
				<ribbonColor>gray</ribbonColor>
			</gitHub>
			<facebookLike/>

		</fluidoSkin>
	</custom>


	<poweredBy>
		<logo name="${project.name}" href="http://site.kubiczak.pl/${project.artifactId}/" img="/images/logo.png"/>
	</poweredBy>

	<body>
		<menu name="${project.name}">
			<item name="README" href="/README.html"/>
		</menu>
		<menu ref="reports"/>
	</body>
</project>
